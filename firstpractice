#include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main()
{
    int z;
    int sum = 0;
    printf("Enter the number:");
    scanf_s("%d", &z);
    if (z <= 0) { printf("Input Error"); return 0; }

    for (int x = 0; x * x * x <= z; x++)
    {
        int sum = 0;
        for (int y = 0; sum <= z; y++)
        {
            sum = x * x * x + y * y * y;
            if (sum == z)
                printf("%d = %d ^ 3 + %d ^ 3\n",z, x, y);

        }
    }
    
    return 0;
}
